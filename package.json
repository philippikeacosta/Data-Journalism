{
  "name": "data-journalism",
  "version": "1.0.0",
  "main": "util/generator.js",
  "dependencies": {
    "d3": "^6.3.1",
    "ejs": "^3.1.5",
    "fs": "0.0.1-security",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "jshint": "^2.12.0",
    "less": "^3.13.0",
    "http-server": "^0.12.3"
  },
  "devDependencies": {},
  "scripts": {
    "precompile": "node util/precompile.js",
    "compile:html": "node util/generator.js",
    "compile:css": "lessc src/LESS/styles.less build/css/styles.css",
    "compile": "npm run compile:css && npm run compile:html",
    "lint": "jshint build/js/scripts.js",
    "test": "jest",
    "document": "jsdoc build/js/scripts.js -d docs",
    "build": "npm run precompile && npm run compile && npm run lint && npm run test && npm run document",
    "build:start": "start http://127.0.0.1:8080 && node ./node_modules/http-server/bin/http-server ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philippikeacosta/Data-Journalism.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/philippikeacosta/Data-Journalism/issues"
  },
  "homepage": "https://github.com/philippikeacosta/Data-Journalism#readme",
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "undef": true,
    "unused": true,
    "varstmt": true,
    "devel": true,
    "browser": true,
    "esversion": 6
  },
  "directories": {
    "test": "test"
  },
  "description": ""
}
